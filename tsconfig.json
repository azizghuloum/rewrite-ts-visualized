{
  "compilerOptions": {
    "noEmit": true,
    "target": "es2016",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "exclude": ["./tests"]
}
